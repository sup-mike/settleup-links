{
  "rewrites": [
    {
      "source": "/.well-known/apple-app-site-association",
      "destination": "/api/well-known/apple-app-site-association"
    }
  ],
  "redirects": [
    {
      "source": "/((?!.well-known).*)",
      "destination": "https://app.settleup.pro/$1",
      "permanent": false
    }
  ]
}
